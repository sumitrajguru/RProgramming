
# 1.c() - Combine function
numVector <- c(2019, 1, 37, 10, 8) 

charVector <- c("a", "b", "c", "d", 89)

# Check what type of vector it is
is.numeric(numVector)
is.integer(numVector)
is.double(numVector)
is.character(numVector)

# 2. seq()
# seq() function is used to generate the sequence of numbers
seqVector <- seq(1,10)
seqVector

# Difference between 1:10 and seq(1,10)
1:10

seqVector <- seq(1, 10, 2)
seqVector